{
  "name": "website-builder-backend",
  "version": "1.0.0",
  "description": "Backend API service for the AI-powered website builder",
  "main": "dist/server.js",
  "type": "module",  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:debug": "tsx watch --inspect=0.0.0.0:9229 src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:setup": "tsx scripts/setup-db.ts",
    "db:validate": "tsx scripts/validate-db.ts",
    "db:studio": "prisma studio",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
